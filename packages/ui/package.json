{
  "name": "@radflow/ui",
  "version": "0.1.0",
  "description": "UI component library for RadFlow",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "@phosphor-icons/react": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "@phosphor-icons/react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@phosphor-icons/react": "^2.1.10",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "tsup": "^8.3.0",
    "typescript": "^5"
  },
  "keywords": [
    "radflow",
    "react",
    "components",
    "ui",
    "design-system"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Radiants-DAO/radflow.git",
    "directory": "packages/ui"
  },
  "sideEffects": false,
  "dependencies": {
    "@radflow/theme-rad-os": "workspace:*",
    "clsx": "^2.0.0"
  }
}
